{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "SESSION_PASSWORD": "@session_password"
  },
  "functions": {
    "app/api/extract/suggest/route.ts": {
      "maxDuration": 30
    },
    "app/api/extract/download/route.ts": {
      "maxDuration": 30
    },
    "app/api/imap/list/route.ts": {
      "maxDuration": 30
    },
    "app/api/imap/fetch/[uid]/route.ts": {
      "maxDuration": 30
    }
  }
}